{
    "extends": [
        "react-app",
        "airbnb",
        "plugin:jsx-a11y/recommended"
    ],
    "plugins": [
        "jsx-a11y"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "classes": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ]
    }
}